{
    "name": "krbotman/driver-line",
    "license": "MIT",
    "description": "LINE driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "LINE"
    ],
    "homepage": "http://github.com/gundamew/botman-driver-line",
    "authors": [
        {
            "name": "Bing-Sheng Chen",
            "email": "gundamew@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "krbotman/botman": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "dev-master",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Line\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Line\\Providers\\LineServiceProvider"
            ]
        }
    }
}
